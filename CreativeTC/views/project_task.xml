<odoo>
    <data>
        <record id="view_todo_form_inherit_date_assigned" model="ir.ui.view">
            <field name="name">todo.form.inherit.date.assigned</field>
            <field name="model">project.task</field>
            <field name="inherit_id" ref="your_module_id.view_todo_form"/> 
            <field name="arch" type="xml">
                <xpath expr="//field[@name='user_ids']" position="after">
                    <field name="date_assigned"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
